<% flash.each do |key, value| %>
  <div class="alert alert-<%= twitterlized_alert_type(key) %> alert-dismissible" role="alert">
    <button class="close" type="button" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
    <%= value %>
  </div>
<% end %>
